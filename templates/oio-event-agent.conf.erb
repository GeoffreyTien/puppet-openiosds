[event-agent]
namespace = <%= @ns %>
user = <%= scope['openiosds::user'] %>
bind_addr = <%= @_bind_addr %>
<% if @workers -%>
workers = <%= @workers %>
<% end -%>
queue_location = <%= @_queue_location %>
retries_per_second = <%= @retries_per_second %>
batch_size = <%= @batch_size %>
acct_update = <%= @acct_update %>
rdir_update = <%= @rdir_update %>
log_facility = <%= @log_facility %>
log_level = <%= @log_level %>
log_name = <%= @_log_name %>
log_address = <%= @log_address %>
syslog_prefix = OIO,<%= @ns %>,<%= @type %>,<%= @num %>
