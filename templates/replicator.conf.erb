{
    "logs" = {
        "access" = "<%= scope['openiosds::logdir'] %>/<%= @ns %>/<%= @type %>-<%= @num %>/<%= @type %>.access",
        "errors" = "<%= scope['openiosds::logdir'] %>/<%= @ns %>/<%= @type %>-<%= @num %>/<%= @type %>.log",
        "level" = "INFO"
    },

    "source" = {
        "oio" = {
            "proxy" = {
                "url" = "<%= @source_oioproxy_url %>",
<% if @source_ecd_url -%>
                "ecd" = "<%= @source_ecd_url %>",
<% end -%>
                "ns" = "<%= @source_ns %>"
            }    
        }
    },
    
    "destination" = {
        "oio" = {
            "proxy" = {
                "url" = "<%= @destination_oioproxy_url %>",
<% if @destination_ecd_url -%>
                "ecd" = "<%= @destination_ecd_url %>",
<% end -%>
                "ns" = "<%= @destination_ns %>"
            }
        }
    },

    "consumer" = {
        "target" = "<%= @consumer_target %>",
        "queue" = "<%= @consumer_queue %>"
    },

    "admin": {
        "bind": "127.0.0.1",
        "port": <%= scope['openiosds::replicator_admin_port'] %>
    }
}
